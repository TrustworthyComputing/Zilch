pubread $t0             # consume next word from public tape and store it to t0
print $t0

secread $t1             # consume next word from auxiliary tape and store it to t1
print $t1

pubseek $t0, 3          # read the 4th word from the public tape and store it to t0
print $t0

secseek $t1, 3          # read the 4th word from the auxiliary tape and store it to t1
print $t1

answer $t0
