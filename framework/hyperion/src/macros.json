{
    "inc": {
        "reg1": "$x",
        "macro": "add $x, $x, 1"
    },
    "dec": {
        "reg1": "$x",
        "macro": "sub $x, $x, 1"
    },
    "pubread": {
        "reg1": "$x",
        "macro": "pubread $x, $x, $x"
    },
    "secread": {
        "reg1": "$x",
        "macro": "secread $x, $x, $x"
    },
    "pubseek": {
        "reg1": "$x",
        "reg2": "$y",
        "macro": "pubseek $x, $y, $y"
    },
    "secseek": {
        "reg1": "$x",
        "reg2": "$y",
        "macro": "secseek $x, $y, $y"
    },
    "print": {
        "reg1": "$x",
        "macro": "print $x, $x, $x"
    },
    "println": {
        "reg1": "$x",
        "macro": "println $x, $x, $x"
    },
    "answer": {
        "reg1": "$x",
        "macro": "answer $x, $x, $x"
    },
    "swap": {
        "comment_": "$z is used as intermediate. This macro swaps x with y",
        "reg1": "$z",
        "reg2": "$x",
        "reg3": "$y",
        "macro" : "move $z, $z, $x
                move $x, $x, $y
                move $y, $y, $z"
    },
    "min": {
        "reg1": "$x",
        "reg2": "$y",
        "reg3": "$z",
        "uses_label" : "true",
        "macro" : "blt $y, $z, __min_label__
                move $x, $x, $z
                j __end_min_label__
                __min_label__
                move $x, $x, $y
                __end_min_label__"
        }
}
